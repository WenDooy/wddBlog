{"remainingRequest":"C:\\wdd\\webstorm\\vue-project\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\wdd\\webstorm\\vue-project\\src\\components\\articleList.vue?vue&type=style&index=0&lang=css&","dependencies":[{"path":"C:\\wdd\\webstorm\\vue-project\\src\\components\\articleList.vue","mtime":1590408040794},{"path":"C:\\wdd\\webstorm\\vue-project\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\wdd\\webstorm\\vue-project\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"C:\\wdd\\webstorm\\vue-project\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"C:\\wdd\\webstorm\\vue-project\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\wdd\\webstorm\\vue-project\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["articleList.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4KA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"articleList.vue","sourceRoot":"src/components","sourcesContent":["<template>\r\n    <el-row class=\"sharelistBox\">\r\n        <div v-if=\"this.$route.name == 'Share' && !this.$route.query.keywords\" class=\"shareTitle\">\r\n<!--            <div class=\"ui label\">-->\r\n<!--                <a :href=\"'/Share?classId=' + classId\">{{className}}</a>-->\r\n<!--            </div>-->\r\n<!--            <ul v-if=\"sonclassList\" class=\"shareclassTwo\">-->\r\n<!--                <li v-for=\"(citem, index) in sonclassList\">-->\r\n<!--                    <a :href=\"'#Share?classId='+classId+'&classtowId'+citem.class_id\" :class=\"citem.class_id==classtowId?'active':''\">-->\r\n<!--                        {{citem.cate_name}}-->\r\n<!--                    </a>-->\r\n<!--                </li>-->\r\n<!--            </ul>-->\r\n        </div>\r\n\r\n        <el-col :span=\"24\" class=\"s-item tcommonBox\" v-for=\"(item, index) in articleList\" :key=\"'article'+index\">\r\n<!--            <span class=\"s-round-date\">-->\r\n<!--                <span class=\"month\" v-html=\"showInitDate(item.create_time, 'month')+'月'\"></span>-->\r\n<!--                <span class=\"day\" v-html=\"showInitDate(item.create_time, 'date')\"></span>-->\r\n<!--            </span>-->\r\n            <header>\r\n                <h1>\r\n                    <a :href=\"'/DetailShare?aid='+item.id\" target=\"_blank\">\r\n                        {{item.title}}\r\n                    </a>\r\n                </h1>\r\n                <h2>\r\n                    <i class=\"el-icon-user myicon\"></i>发表于\r\n                    <i class=\"el-icon-date myicon\"></i>\r\n                    <span v-html=\"showInitDate(item.create_time, 'all')\">\r\n                        {{showInitDate(item.create_time, 'all')}}\r\n                    </span>\r\n                    <i class=\"el-icon-chat-line-round myicon\"></i>{{item.comment}}条\r\n                    <i class=\"el-icon-postcard myicon\"></i>\r\n                    <a :href=\"'/Share?classId='+item.class_id\">{{item.cate_name}}</a>\r\n                </h2>\r\n            </header>\r\n            <div class=\"article-content\">\r\n                <p style=\"text-indent: 2em;\">\r\n                    {{item.description}}\r\n                </p>\r\n                <p style=\"max-height: 300px; overflow: hidden; text-align: center;\">\r\n                    <img :src=\"item.image\" alt=\"img\" class=\"maxW\">\r\n                </p>\r\n            </div>\r\n            <div class=\"viewdetail\">\r\n                <a class=\"tcolors-bg\" :href=\"'/DetailShare?aid='+item.id\" target=\"_blank\">\r\n                    阅读全文\r\n                </a>\r\n            </div>\r\n        </el-col>\r\n        <el-col class=\"viewmore\">\r\n            <a href=\"javasript:void(0);\" class=\"tcolors-bg\" v-show=\"hasMore\" @click=\"addMoreFun\">点击加载</a>\r\n            <a href=\"javasript:void(0);\" class=\"tcolors-bg\" v-show=\"!hasMore\">无更多数据</a>\r\n        </el-col>\r\n    </el-row>\r\n</template>\r\n\r\n<script>\r\n    import {ArtClassData, ShowArticleAll} from '../utils/server'\r\n    export default {\r\n        name: \"articleList\",\r\n        data(){\r\n            return {\r\n                hasMore: false,\r\n                articleList: [{\r\n                    id: 0,\r\n                    title: '测试',\r\n                    create_time: '2014-04-23 18:55:49:123',\r\n                    comment: 'test',\r\n                    cate_name: 'hello world',\r\n                    description: '肯定是卡卡反馈会发出阿八福晋能否阿克苏发卡机从北京',\r\n                    image: '../static/img/headbg05'\r\n                },{\r\n                    id: 1,\r\n                    title: '测试',\r\n                    create_time: '2014-04-23 18:55:49:123',\r\n                    comment: 'test',\r\n                    cate_name: 'hello world',\r\n                    description: '肯定是卡卡反馈会发出阿八福晋能否阿克苏发卡机从北京',\r\n                    image: '../static/img/headbg05'\r\n                }],\r\n                artId: 0,\r\n                classId: 0,\r\n                keywords: '',\r\n                level: 0,\r\n                sendId: 0,\r\n                className: '',\r\n                shareClass: [\r\n                    {classId:1,name:'技术分享'},\r\n                    {classId:2,name:'闲言碎语'},\r\n                    {classId:3,name:'事件簿'},\r\n                    {classId:4,name:'创作集'}\r\n                ]\r\n            }\r\n        },\r\n        methods:{\r\n            showInitDate (oldDate, full) {\r\n                console.log(oldDate, new Date(oldDate));\r\n                let newDate = new Date(oldDate);//oldDate '2014-04-23 18:55:49:123'\r\n                let year = newDate.getFullYear();\r\n                let month = newDate.getMonth()<9 ? '0'+(newDate.getMonth()+1) : newDate.getMonth()+1;\r\n                let date = newDate.getDate()<10 ? '0'+(newDate.getDate()) : newDate.getDate();\r\n                if(full == 'all'){\r\n                    let time = oldDate.split(' ')[0];\r\n                    return time.split('-')[0]+'年'+time.split('-')[1]+'月'+time.split('-')[2]+'日';\r\n                }\r\n                else if(full == 'year'){\r\n                    return year;\r\n                }\r\n                else if(full == 'month'){\r\n                    return month;\r\n                }\r\n                else if(full == 'Date'){\r\n                    return date;\r\n                }\r\n                else if(full == 'newDate'){\r\n                    return year+'年'+month+'月'+date+'日';\r\n                }\r\n            },\r\n            getArticleList (initpage) {\r\n                this.keywords = this.$store.state.keywords;\r\n                this.classId = (this.$route.query.classId == undefined ? 0 : parseInt(this.$route.query.classId));\r\n                this.sendId = this.classId;\r\n                this.level = this.keywords ? 0 : 1;\r\n\r\n                ArtClassData(function (msg) {\r\n                    this.shareClass = msg;\r\n                })\r\n\r\n                //显示当前的分类名称\r\n                for(let i=0; i<this.shareClass.length; i++){\r\n                    if(this.classId == this.shareClass[i].class_id){\r\n                        this.className = this.shareClass[i].name;\r\n                    }\r\n                }\r\n\r\n                //初始化， 文章id以0开始\r\n                this.artId = initpage ? 0 : this.artId;\r\n                ShowArticleAll(this.artId, this.sendId, this.keywords, this.level, (result)=>{\r\n                    if(result.code == 1001){\r\n                        let msg = result.data;\r\n                        if(msg.length>0 && msg.length<10){\r\n                            this.hasMore = false;\r\n                        }\r\n                        else{\r\n                            this.hasMore = true;\r\n                        }\r\n                    }else{\r\n                         this.hasMore = false;\r\n                         this.articleList = initpage ? [] : this.articleList;\r\n                    }\r\n                })\r\n            },\r\n            addMoreFun() {\r\n                this.getArticleList(false);\r\n            },\r\n            routeChange() {\r\n                this.getArticleList(true);\r\n            }\r\n        },\r\n        watch: {\r\n            '$route': 'routeChange',\r\n            '$store.state.keywords': 'routeChange'\r\n        },\r\n        created(){\r\n            this.routeChange();\r\n        }\r\n    }\r\n</script>\r\n\r\n<style >\r\n    /*分享标题*/\r\n    .shareTitle{\r\n        margin-bottom: 40px;\r\n        position: relative;\r\n        border-radius: 5px;\r\n        background: #fff;\r\n        padding:15px;\r\n    }\r\n    .shareclassTwo{\r\n        width:100%;\r\n    }\r\n    .shareclassTwo li{\r\n        display: inline-block;\r\n    }\r\n    .shareclassTwo li a{\r\n        display: inline-block;\r\n        padding:3px 7px;\r\n        margin:5px 10px;\r\n        color:#fff;\r\n        border-radius: 4px;\r\n        background: #64609E;\r\n        border: 1px solid #64609E;\r\n        transition: transform 0.2s linear;\r\n        -webkit-transition: transform 0.2s linear;\r\n    }\r\n    .shareclassTwo li a:hover{\r\n        transform: translate(0,-3px);\r\n        -webkit-transform: translate(0,-3px);\r\n    }\r\n    .shareclassTwo li a.active{\r\n        background: #fff;\r\n        color:#64609E;\r\n\r\n    }\r\n    /*文章列表*/\r\n    .sharelistBox{\r\n        transition: all 0.5s ease-out;\r\n        font-size: 15px;\r\n        margin-top: 20px;\r\n    }\r\n    .viewdetail a {\r\n        background: #555555;\r\n    }\r\n    .viewmore a{\r\n        background: #555555;\r\n    }\r\n    .myicon{\r\n        margin-left: 10px;\r\n        margin-right: 2px;\r\n    }\r\n\r\n</style>"]}]}