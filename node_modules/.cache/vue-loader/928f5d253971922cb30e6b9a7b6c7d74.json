     this.textarea = '';\r\n                this.routeChange();\r\n                let reset = setTimeout(function () {\r\n                                this.sendTip = '评论';\r\n                                clearTimeout(reset);\r\n                            }, 1000)\r\n\r\n            },\r\n            getCreateTime() {\r\n                // 比如需要这样的格式 yyyy-MM-dd hh:mm:ss\r\n                let date = new Date();\r\n                let year = date.getFullYear() + '-';\r\n                let month = (date.getMonth() + 1 < 10 ? '0' + (date.getMonth() + 1) : date.getMonth() + 1) + '-';\r\n                let day = date.getDate() + ' ';\r\n                let hour = date.getHours() + ':';\r\n                let min = date.getMinutes() + ':';\r\n                let sec = date.getSeconds();\r\n                return year + month + day + hour + min + sec;\r\n            },\r\n            analyzeEmoji(commentMsg){\r\n                let pattern1 = /\\[[\\u4e00-\\u9fa5]+\\]/g;\r\n                let pattern2 = /\\[[\\u4e00-\\u9fa5]+\\]/;\r\n                let content = commentMsg.match(pattern1);\r\n                let str = commentMsg;\r\n                let img_src = '';\r\n                if(content){\r\n                    for(let i=0; i<content.length; i++){\r\n                        for(let j=0; j<this.emojiList.length; j++){\r\n                            if(\"[\"+this.emojiList[j].title+\"]\" === content[i]){\r\n                                img_src = this.emojiList[j].url;\r\n                                break;\r\n                            }\r\n                        }\r\n                        str = str.replace(pattern2, '<img src=\"'+img_src+'\">');\r\n                    }\r\n                }\r\n                return str;\r\n            },\r\n            showCommentList(){\r\n                this.artId = this.$route.query.aid === undefined ? 0 : parseInt(this.$route.query.aid);\r\n                let article = this.$store.getters.articleList.find